import matplotlib.pyplot as plt

# === Get feature importances from trained model
feature_names = X.columns
importances = clf.feature_importances_

# === Sort and plot
plt.figure(figsize=(7, 5))
plt.barh(feature_names, importances, color="slateblue", edgecolor="black")
plt.title("ğŸ“Š Trait Importance for Flip Prediction")
plt.xlabel("Relative Importance")
plt.tight_layout()
plt.show()

Code 25
# === Input custom trait deltas
custom_validator = pd.DataFrame({
    "Echo Î”": [-0.08],       # echo collapse
    "Curvature Î”": [0.002],  # mild stabilizer
    "Tension Î”": [0.0003],   # slight biome pull
    "Phase Î”": [10.5]        # strong divergence
})

# === Predict flip outcome
prediction = clf.predict(custom_validator)[0]
result = "ğŸ” Species Flip" if prediction == 1 else "ğŸ§± Identity Held"

print(f"ğŸ¯ Prediction Result: {result}")

Code 26
import numpy as np
import matplotlib.pyplot as plt
from matplotlib.colors import ListedColormap
from sklearn.ensemble import RandomForestClassifier

# === Use trained classifier from earlier
# Define grid range for Echo Î” and Phase Î”
echo_range = np.linspace(-0.1, 0.1, 100)
phase_range = np.linspace(-5, 15, 100)
echo_grid, phase_grid = np.meshgrid(echo_range, phase_range)

# === Create test grid of trait combinations
test_grid = pd.DataFrame({
    "Echo Î”": echo_grid.ravel(),
    "Curvature Î”": [0.0] * echo_grid.size,    # fixed
    "Tension Î”": [0.0] * echo_grid.size,      # fixed
    "Phase Î”": phase_grid.ravel()
})

# === Predict flips over grid
flip_preds = clf.predict(test_grid).reshape(echo_grid.shape)

# === Plot decision boundary
plt.figure(figsize=(8, 6))
cmap = ListedColormap(["lightgrey", "darkorange"])
plt.contourf(echo_grid, phase_grid, flip_preds, cmap=cmap, alpha=0.6)
plt.xlabel("Echo Î”")
plt.ylabel("Phase Î”")
plt.title("ğŸŒŒ Flip Zone Map â€” Species Change Boundary")
plt.grid(True)
plt.tight_layout()
plt.show()

